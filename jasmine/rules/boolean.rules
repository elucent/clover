inot_negate_lt(not any (is_lt T:int x:? y:?)):
    (is_ge T x:? y:?)

inot_negate_le(not any (is_le T:int x:? y:?)):
    (is_gt T x:? y:?)

inot_negate_gt(not any (is_gt T:int x:? y:?)):
    (is_le T x:? y:?)

inot_negate_ge(not any (is_ge T:int x:? y:?)):
    (is_lt T x:? y:?)

inot_negate_eq(not any (is_eq T:int x:? y:?)):
    (is_ne T x:? y:?)

inot_negate_ne(not any (is_ne T:int x:? y:?)):
    (is_ne T x:? y:?)

double_not_elimination(not T:any (not T x:?)):
    x:?

inot_fold(not T:int x:i):
    [!x]:i
